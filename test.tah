fun errorTest() {
    print "In errorTest";
    var a = 5;
    a(); // This should cause a runtime error
}

// Test nested function calls
fun nestedFunction() {
    print "Inside nestedFunction";
    print "Calling errorTest";
    errorTest(); // This should cause a runtime error and show the call stack
}

print "Calling nestedFunction...";
nestedFunction();
